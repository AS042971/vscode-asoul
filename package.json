{
    "name": "asoul",
    "displayName": "A-SOUL鼓励师",
    "description": "写代码不再孤单，有A-SOUL与你同在",
    "version": "0.3.1",
    "publisher": "as042971",
    "icon": "logo.jpg",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "A-SOUL",
        "提醒",
        "鼓励"
    ],
    "bugs": {
        "url": "https://github.com/as042971/vscode-asoul/issues",
        "email": "as042971@outlook.com"
    },
    "homepage": "https://github.com/as042971/vscode-asoul/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/as042971/vscode-asoul.git"
    },
    "activationEvents": [
        "onCommand:asoul.showReminderView",
        "onCommand:asoul.openGallery",
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "asoul.showReminderView",
                "title": "打开提醒页面",
                "category": "asoul"
            },
            {
                "command": "asoul.openGallery",
                "title": "打开二创画廊 (默认浏览器)",
                "category": "asoul"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "A-SOUL",
            "properties": {
                "asoul.reminderViewIntervalInMinutes": {
                    "type": "number",
                    "default": 60,
                    "description": "展示提醒页面的时间间隔（分钟）"
                },
                "asoul.title": {
                    "type": "string",
                    "default": "亲爱的一个魂儿，代码写久了，该休息啦~",
                    "description": "默认的页面内展示文字"
                },
                "asoul.titleAva": {
                    "type": "string",
                    "default": "亲爱的顶碗人，代码写久了，该休息啦~ 要天天开心呀",
                    "description": "🎸向晚🎸相关图片的页面内展示文字"
                },
                "asoul.titleBella": {
                    "type": "string",
                    "default": "亲爱的贝极星，代码写久了，该休息啦~ 要多多运动，身体健康吖",
                    "description": "💃贝拉💃相关图片的页面内展示文字"
                },
                "asoul.titleCarol": {
                    "type": "string",
                    "default": "亲爱的皇珈骑士，代码写久了，该休息啦~ 今天也是酷酷的捏",
                    "description": "🎤珈乐🎤相关图片的页面内展示文字"
                },
                "asoul.titleDiana": {
                    "type": "string",
                    "default": "亲爱的嘉心糖，代码写久了，该休息啦~ 要注意劳逸结合喔",
                    "description": "🎀嘉然🎀相关图片的页面内展示文字"
                },
                "asoul.titleEileen": {
                    "type": "string",
                    "default": "亲爱的奶淇琳，代码写久了，该休息啦~ 想要陪你双向奔赴",
                    "description": "💄乃琳💄相关图片的页面内展示文字"
                },
                "asoul.type": {
                    "type": "string",
                    "default": "random",
                    "enum": [
                        "url",
                        "random",
                        "default",
                        "niuniu",
                        "cao"
                    ],
                    "enumDescriptions": [
                        "自定义网图",
                        "随机二创",
                        "内置图",
                        "勇敢牛牛，不怕困难！",
                        "狂暴鸿儒"
                    ],
                    "description": "图片来源"
                },
                "asoul.customImages": {
                    "type": "array",
                    "default": [],
                    "description": "用户设置图片地址"
                },
                "asoul.notification": {
                    "type": "boolean",
                    "default": false,
                    "description": "防社死模式：通过右下角消息框提醒休息，而非直接弹出页面"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "axios": "^0.21.1"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^3.5.1",
        "vscode": "^1.1.21"
    }
}
